{
    "name": "codebar-ag/laravel-default",
    "description": "Boilerplate integration for Laravel Projects at codebar Solutions AG.",
    "keywords": [
        "laravel",
        "codebar-ag",
        "laravel-default"
    ],
    "homepage": "https://github.com/codebar-ag/laravel-default",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Fix",
            "email": "sebastian.fix@codebar.ch",
            "homepage": "https://www.codebar.ch",
            "role": "Software Engineer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-ray": "^1.26"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "CodebarAg\\LaravelDefault\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodebarAg\\LaravelDefault\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodebarAg\\LaravelDefault\\LaravelDefaultServiceProvider"
            ],
            "aliases": {
                "Money": "CodebarAg\\LaravelDefault\\Facades\\Money"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
